{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "button-type": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "height: fit-content",
          "backdrop-filter"
        ]
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}